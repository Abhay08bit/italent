    color: #dc3545;
    font-size: 14px;
    position: relative;
    top: 3px;
}

.about_us_desc,
.mission_desc {
    font-size: 18px;
    line-height: 1.33;
    letter-spacing: 0.36px;
    text-align: left;
    color: rgba(17, 17, 17, 0.5);
    margin-top: 50px;
}

.about_us_tab li {
    width: 50%;
}

.about_us_tab li a {
    font-size: 18px;
    font-weight: bold;
    line-height: 2;
    text-align: center;
    color: #181818;
    padding: 0 13px;
}

.about_us_tab li a.active {
    color: #ff6158;
    border-bottom: 1px solid #ff6057;

}

.red_color {
    color: #ff6158;
}

textarea {
    resize: none;
}

#map {
    height: 320px;
    width: 528px;
}

.address img {
    float: left;
    vertical-align: middle;
    padding-right: 1vw;
}

.address a {
    font-size: 16px;
    color: #111111;
}

.address {
    padding-bottom: 15px;
}

.contact_us_add {
    margin-top: 5px;
}

.contactus-wrapper {
    height: 100%;
}

.contactus-wrapper .contact-details {
    align-content: flex-end;
    margin-bottom: -16px;
}

.get-in-touch {
    text-align: center;
    position: relative;
    display: block;
    margin: 4% auto;
}

.get-in-touch p {
    font-size: 20px;
}

.get-in-touch span {
    font-size: 16px;
    color: rgba(17, 17, 17, 0.5);
}

.get-in-touch-button {
    position: relative;
    display: block;
}

.get-in-touch-button button {
    margin: 2% auto;
}

.country {
    margin-bottom: 16px;
    display: flex;
    align-items: center;
}

.contactus-wrapper p span {
    display: block;
}

.country span {
    margin-left: 20px;
    font-size: 16px;
}

.contact-footer {
    font-size: 14px;
    color: rgba(17, 17, 17, 0.7);
}

.contact-card-desc {
    text-align: center;
}

.contact-wraper {
    margin-top: 40px;
}

/* form css */
.contact-form .login-form {
    display: flex;
    flex-wrap: wrap;
    margin-top: 0px;
}

.contact-form .login-form .form-group {
    width: 100%;
    max-width: 100%;
}

.contact-form .login-form .form-name-main {
    display: flex;
    flex-wrap: wrap;
    width: 100%;
    justify-content: space-between;
}

.contact-form .login-form .form-name-main .form-group {
    width: 48%;
    max-width: 48%;
}

.login-form .btn_div {
    width: 100%;
}

#designation {
    height: 40px;
}

/* Pagination */

.pagination {
    display: flex;
    list-style: none;
    padding: 0;
}

.pagination li {
    margin-right: 5px;
}

.pagination li button {
    background-color: #c6c6c6;
    color: white;
    border: none;
    padding: 8px 12px;
    border-radius: 5px;
    cursor: pointer;
    transition: background-color 0.3s ease;
}

.pagination li button:hover {
    background-color: #7b7b7b;
}

.pagination li.active button {
    background-color: #03232d;
}

