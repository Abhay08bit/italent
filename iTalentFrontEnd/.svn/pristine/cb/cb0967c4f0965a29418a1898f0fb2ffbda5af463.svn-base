/* Modal Styles */
.modal {
    position: fixed;
    top: 0;
    left: 0;
    width: 100%;
    height: 100%;
    background-color: rgba(0, 0, 0, 0.5);
    display: flex;
    justify-content: center;
    align-items: center;
}

.modal-content {
    background-color: white;
    padding: 20px;
    border-radius: 5px;
    max-width: 90%;
    /* Adjust the max-width as needed */
    overflow: auto;
    /* Enable vertical scrolling */
}

.modal-scrollable-content {
    max-height: 70vh;
    /* Adjust the max-height as needed */
    overflow-y: auto;
    /* Enable vertical scrolling */
}

.close {
    color: #aaa;
    float: right;
    font-size: 28px;
    font-weight: bold;
    cursor: pointer;
}

.close:hover,
.close:focus {
    color: black;
    text-decoration: none;
    cursor: pointer;
}


.modal-close {
    display: flex;
    margin-right: 0px;
    font-size: 30px;
    cursor: pointer;
    clear: both;
}

