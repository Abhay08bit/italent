.arrow-highlight {
    position: relative;
    margin: 0 0.5em;
    padding: 0 0.2em;
}

.arrow-highlight:before {
    content: "";
    z-index: -1;
    left: -0.5em;
    top: 0.1em;
    border-width: 0.5em;
    border-style: solid;
    border-color: orange;
    position: absolute;
    width: calc(100% - 0.5em);
    border-left-color: transparent;
}

.arrow-highlight:after {
    content: "";
    z-index: -1;
    right: 0;
    top: 0.1em;
    border-width: 0.5em;
    border-style: solid;
    border-color: orange;
    position: absolute;
    border-top-color: transparent;
    border-bottom-color: transparent;
    border-left-color: transparent;
    transform: rotate(180deg);
    transform-origin: center right;
}

