,eAaI;AAAK,QAAA,SAAS,EAAC,mCAAf;AAAmD,QAAA,KAAK,EAAE;AAACuC,UAAAA,UAAU,EAAE;AAAb,SAA1D;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,sBACI;AAAQ,QAAA,KAAK,EAAEE,EAAE,CAACtC,GAAlB;AAAuB,QAAA,QAAQ,EAAGO,EAAD,IAAQ;AAAE,eAAKa,kBAAL,CAAwBb,EAAxB,EAA4BJ,GAA5B;AAAkC,SAA7E;AAA+E,QAAA,IAAI,EAAE,uBAAuBmC,EAAE,CAACzC,IAA/G;AAAqH,QAAA,SAAS,EAAC,eAA/H;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,sBACI;AAAQ,QAAA,KAAK,EAAC,KAAd;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,eADJ,eAEI;AAAQ,QAAA,KAAK,EAAC,KAAd;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,eAFJ,eAGI;AAAQ,QAAA,KAAK,EAAC,KAAd;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,eAHJ,eAII;AAAQ,QAAA,KAAK,EAAC,IAAd;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,cAJJ,CADJ,CAbJ,eAqBI;AAAK,QAAA,SAAS,EAAC,0BAAf;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,SACKM,GAAG,GAAG,CAAN,iBAAW;AAAK,QAAA,SAAS,EAAC,gCAAf;AAAgD,QAAA,OAAO,EAAE,KAAKT,SAA9D;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,sBAA0E;AAAK,QAAA,GAAG,EAAEd,QAAV;AAAoB,QAAA,GAAG,EAAC,UAAxB;AAAmC,QAAA,SAAS,EAAC,uBAA7C;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,QAA1E,CADhB,EAEKuB,GAAG,KAAK,CAAR,iBAAa;AAAK,QAAA,SAAS,EAAC,gCAAf;AAAgD,QAAA,OAAO,EAAE,MAAM,KAAKD,YAAL,CAAkBC,GAAlB,CAA/D;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,sBAAuF;AAAK,QAAA,GAAG,EAAEtB,UAAV;AAAsB,QAAA,GAAG,EAAC,YAA1B;AAAuC,QAAA,SAAS,EAAC,uBAAjD;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,QAAvF,CAFlB,CArBJ,CADJ;AA4BH,KA7BD,CAbR,eA4CI;AAAK,MAAA,SAAS,EAAC,kCAAf;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,oBACI;AAAO,MAAA,IAAI,EAAC,QAAZ;AAAqB,MAAA,IAAI,EAAC,OAA1B;AAAkC,MAAA,OAAO,EAAE,KAAKwC,SAAhD;AAA2D,MAAA,KAAK,EAAC,OAAjE;AAAyE,MAAA,SAAS,EAAC,2BAAnF;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,MADJ,eAEI;AAAO,MAAA,IAAI,EAAC,QAAZ;AAAqB,MAAA,OAAO,EAAE,KAAKC,YAAnC;AAAiD,MAAA,KAAK,EAAE;AAAEiB,QAAAA,MAAM,EAAE;AAAV,OAAxD;AAA8E,MAAA,IAAI,EAAC,MAAnF;AAA0F,MAAA,KAAK,EAAC,MAAhG;AAAuG,MAAA,SAAS,EAAC,+CAAjH;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,MAFJ,CA5CJ,CAFJ,CADJ,CAHJ,CAFJ,CADJ,CAFJ,eAiEI,oBAAC,MAAD;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,MAjEJ,CADJ;AAqEH;;AAhLoC;;AAmLzC,eAAe5D,UAAU,CAACS,UAAD,CAAzB","sourcesContent":["import React from 'react';\r\nimport { withRouter } from 'react-router-dom';\r\nimport addImage from '../../assets/images/add.png';\r\nimport trashImage from '../../assets/images/trash.png';\r\nimport Header from '../common/header/header';\r\nimport Footer from '../common/footer/footer';\r\nimport _ from 'lodash';\r\nimport { ToastContainer, toast } from 'react-toastify';\r\nimport 'react-toastify/dist/ReactToastify.css';\r\nimport localforage from 'localforage';\r\n\r\n\r\nclass FindTalent extends React.Component {\r\n    get initialState() {\r\n        return [\r\n            {\r\n                value: \"\",\r\n                name: \"talent-1\",\r\n                count: 1,\r\n                search_key: \"Primary Skills\",\r\n                Exp: \"1-3\"\r\n            }\r\n        ]\r\n    }\r\n\r\n    state = {\r\n        inputCount: this.initialState\r\n    }\r\n\r\n    inputTotal = 1;\r\n\r\n    addInputs = () => {\r\n        if(this.inputTotal >= 4) return;\r\n        this.inputTotal++;\r\n        const inputObj = { value: \"\", name: `talent-${this.inputTotal}`, count: 1, search_key: \"Primary Skills\", Exp: \"1-3\" }\r\n        this.setState({ inputCount: [...this.state.inputCount, inputObj] });\r\n    }\r\n\r\n    removeInputs = (key) => {\r\n        this.inputTotal--;\r\n        this.setState({\r\n            inputCount: this.state.inputCount.filter((_, i) => i !== key)\r\n        });\r\n    }\r\n\r\n    onInputChange = (ev, index) => {\r\n        const data = this.state.inputCount;\r\n        data[index].value = ev.target.value;\r\n        this.setState({\r\n            inputCount: data,\r\n            serverError: \"\"\r\n        })\r\n    }\r\n\r\n    componentDidMount() {\r\n        this.notify();\r\n        this.existingData();\r\n    }\r\n\r\n    componentDidUpdate() {\r\n        this.notify();\r\n        this.existingData();\r\n    }\r\n\r\n    existingData = async () => {\r\n        let data = await localforage.getItem(\"find-talent\");\r\n        if(data) {\r\n            this.setState({\r\n                inputCount: data\r\n            })\r\n            localforage.removeItem(\"find-talent\");\r\n        }        \r\n    }\r\n\r\n    \r\n    notify = async () => {\r\n        let hasError = await localforage.getItem(\"team-error\");\r\n        if(hasError) {\r\n            toast.error(hasError);\r\n            localforage.removeItem(\"team-error\");\r\n        }        \r\n    };\r\n\r\n    onSelectChange = (ev, index) => {\r\n        const data = this.state.inputCount;\r\n        data[index].count = parseInt(ev.target.value);\r\n        this.setState({\r\n            inputCount: data\r\n        })\r\n    }\r\n\r\n    onExperienceChange =(ev, index) => {\r\n        const data = this.state.inputCount;\r\n        data[index].Exp = ev.target.value;\r\n        this.setState({\r\n            inputCount: data\r\n        })\r\n    }\r\n\r\n    resetForm = () => {\r\n        this.setState({\r\n            inputCount: this.initialState\r\n        });\r\n    }\r\n\r\n    searchTalent = () => {\r\n        if (_.isEqual((this.state.inputCount).sort(), (this.initialState).sort())) {\r\n            this.setState({\r\n                serverError: \"Atleast one member required!!\"\r\n            });\r\n        } else {\r\n            let _this = this;\r\n            localforage.setItem('find-talent', this.state.inputCount, function (err) {\r\n                _this.props.history.push(\"/team\");\r\n            });\r\n        }\r\n    }\r\n\r\n    render() {\r\n        return (\r\n            <React.Fragment>\r\n                <ToastContainer />\r\n                <div className={`container-fluid header-background-color`} style={{ position: 'relative' }}>\r\n                    <div className=\"container\">\r\n                        <Header />\r\n                        <section className=\"section-1\">\r\n                            <h1 className=\"text-center main_heading\">Find Your Talented Team</h1>\r\n                            <p className=\"text-center main_sub_heading\">We are the largest, globally-distributed network of top business, design, and technology talent, ready to tackle your most important initiatives.</p>\r\n                            <div>\r\n                                <form className=\"searh_form\">\r\n                                    <div className=\"text-center text-danger error_msg\"><span className=\"server-error\">{this.state.serverError}</span></div>\r\n                                    <div className=\"form-group\">\r\n                                        <div className=\"row search_form_div\">\r\n                                            <div className=\"col-12 col-md-6 col-lg-5\">\r\n                                                <span>Skills</span>\r\n                                            </div>\r\n                                            <div className=\"col-12 col-md-2 col-lg-1 padding0\" style={{left: \"-8px\"}}>\r\n                                                <span>Members</span>\r\n                                            </div>\r\n                                            <div className=\"col-12 col-md-2 col-lg-4 padding0\" style={{marginLeft: \"25px\"}}>\r\n                                                <span>Experience (years)</span>\r\n                                            </div>\r\n                                        </div>\r\n                                        {\r\n                                            this.state.inputCount.map((el, key) => {\r\n                                                return (\r\n                                                    <div className=\"row search_form_div\" key={key}>\r\n                                                        <div className=\"col-12 col-md-7 col-lg-5\">\r\n                                                            <input type=\"text\" id= \"designation\" className=\"form-control\" onChange={(ev) => { this.onInputChange(ev, key) }} name={el.name} value={el.value} />\r\n                                                        </div>\r\n                                                        <div className=\"col-12 col-md-2 col-lg-1 padding0\">\r\n                                                            <select value={el.count} onChange={(ev) => { this.onSelectChange(ev, key) }} name={\"select-\" + el.name} className=\"custom-select\">\r\n                                                                <option value={1}>1</option>\r\n                                                                <option value={2}>2</option>\r\n                                                                <option value={3}>3</option>\r\n                                                                <option value={4}>4</option>\r\n                                                                <option value={5}>5</option>\r\n                                                            </select>\r\n                                                        </div>\r\n                                                        <div className=\"col-12 col-md-2 col-lg-2 padding0\" style={{marginLeft: \"40px\"}}>\r\n                                                            <select value={el.Exp} onChange={(ev) => { this.onExperienceChange(ev, key) }} name={\"select-experience-\" + el.name} className=\"custom-select\">\r\n                                                                <option value=\"1-3\">1-3</option>\r\n                                                                <option value=\"3-5\">3-5</option>\r\n                                                                <option value=\"5-8\">5-8</option>\r\n                                                                <option value=\"8+\">8+</option>\r\n                                                            </select>\r\n                                                        </div>\r\n                                                        <div className=\"col-12 col-md-3 col-lg-2\">\r\n                                                            {key < 3 && <div className=\"icon_background display-inline\" onClick={this.addInputs} ><img src={addImage} alt=\"addImage\" className=\"img-fluid icons_width\" /></div>}\r\n                                                            {key !== 0 && <div className=\"icon_background display-inline\" onClick={() => this.removeInputs(key)}><img src={trashImage} alt=\"trashImage\" className=\"img-fluid icons_width\" /></div>}\r\n                                                        </div>\r\n                                                    </div>\r\n                                                )\r\n                                            })\r\n                                        }\r\n                                        <div className=\"text-center testonomial_btns_div\">\r\n                                            <input type=\"button\" name=\"reset\" onClick={this.resetForm} value=\"Reset\" className=\"btn btn-primary reset_btn\" />\r\n                                            <input type=\"button\" onClick={this.searchTalent} style={{ margin: \"0 10px\" }} name=\"find\" value=\"Find\" className=\"btn btn-primary btn-orange-color my-2 my-sm-0\" />\r\n                                        </div>\r\n                                    </div>\r\n                                </form>\r\n                            </div>\r\n                        </section>\r\n                    </div>\r\n                </div>\r\n                <Footer />\r\n            </React.Fragment>\r\n        );\r\n    }\r\n}\r\n\r\nexport default withRouter(FindTalent);"]},"metadata":{},"sourceType":"module"}